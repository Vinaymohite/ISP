.dp-highlighter.nogutter .bar
{
	padding-left: 0px;
}

.dp-highlighter ol
{
	list-style: decimal; /* for ie */
	background-color: #fff;
	margin: 0px 1px 1px 45px !important; /* 1px bottom margin seems to fix occasional Firefox scrolling */
